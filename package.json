{
  "name": "autonomia-backend",
  "version": "0.1.0",
  "description": "Backend para o projeto autonom.ia",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "node ./shared/migrations/migrate-knex.js",
    "migrate:status": "node ./shared/migrations/migrate-knex.js --status",
    "migrate:rollback": "node ./shared/migrations/migrate-knex.js --rollback",
    "deploy:api": "serverless deploy --config serverless-api.yml",
    "deploy:api:prod": "serverless deploy --config serverless-api.yml --stage prod",
    "update:ssm": "node ./scripts/update-ssm-parameters.js",
    "update:ssm:prod": "node ./scripts/update-ssm-parameters.js --stage=prod",
    "dev": "node dev-manager.js",
    "dev:start": "node dev-manager.js full-start",
    "dev:test": "node dev-manager.js test",
    "dev:reset": "node dev-manager.js reset",
    "start-project": "node start-project.js",
    "start-all": "start-project.bat",
    "quick-start": "dev.bat",
    "start-saas": "start-saas.bat",
    "start-auth": "start-auth.bat",
    "test-auth": "cd api/auth && node test-login.js",
    "validate": "node validate-environment.js",
    "populate-accounts": "node populate-accounts.js",
    "test-accounts": "node test-accounts-api.js",
    "test-campaigns": "node test-campaigns-templates.js",
    "debug-accounts": "node test-frontend-request.js",
    "monitor": "node monitor-requests.js",
    "kill-all": "kill-all.bat",
    "start-frontend": "start-frontend.bat"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.840.0",
    "axios": "^1.6.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "ioredis": "^5.6.1",
    "knex": "^2.5.1",
    "libphonenumber-js": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "serverless": "^3.35.2",
    "serverless-domain-manager": "^8.0.0",
    "serverless-dotenv-plugin": "^6.0.0"
  }
}
